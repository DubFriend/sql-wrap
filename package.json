{
  "name": "sql-wrap",
  "version": "1.3.3",
  "description": "Makes working with sql easy",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:DubFriend/sql-wrap.git"
  },
  "scripts": {
    "prepublish": "gulp build",
    "test": "gulp test",
    "init":
      "docker-compose --file ./deploy/docker-compose.yml stop && docker-compose --file ./deploy/docker-compose.yml rm -f  && docker-compose --file ./deploy/docker-compose.yml build && docker-compose --file ./deploy/docker-compose.yml up -d && docker-compose --file ./deploy/docker-compose.yml logs",
    "update-install-schema": "bash ./deploy/update-install-schema.sh",
    "flow": "flow",
    "flow-typed": "flow-typed install"
  },
  "author": "Brian Detering",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.4",
    "sqlstring": "^2.2.0",
    "squel": "^5.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "del": "^3.0.0",
    "eslint": "^4.3.0",
    "eslint-config-prettier": "^2.3.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-prettier": "^2.1.2",
    "flow-bin": "^0.74.0",
    "flow-copy-source": "^2.0.0",
    "flow-typed": "^2.4.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.1",
    "gulp-mocha": "^6.0.0",
    "mocha": "^5.2.0",
    "mysql": "^2.15.0",
    "prettier": "^1.13.4",
    "sinon": "^5.1.0",
    "yargs": "^11.0.0"
  }
}
